		<typeAlias alias="search" type="com.morackmorack.mvc.common.Search"/>	
	</typeAliases>

	<mappers>
		<mapper resource="sql/UserMapper.xml"/> 
		 <mapper resource="sql/MeetMapper.xml"/> 
		 <mapper resource="sql/BusinessMapper.xml"/> 
		 <mapper resource="sql/FriendMapper.xml"/> 
		 <mapper resource="sql/NotifyMapper.xml"/>
		 <mapper resource="sql/CommunityMapper.xml"/> 
		 <mapper resource="sql/OffMeetMapper.xml"/> 
		 <mapper resource="sql/MessageMapper.xml"/> 
